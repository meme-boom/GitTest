public class Bus12 {

	
	public static void main(String[] args) {

	
		int[] coinUnit = {500,100,50,10};
		int money =2680;
		boolean a= true;
		int sum=1;
		
		for(int i=0; i<coinUnit.length; i++) {
			while(a=true) {
				money-=coinUnit[i];
				if(money>=coinUnit[i])
					sum++;
				else
					a=false;
				System.out.println(sum);
			}
				
			
		}
		
}
}