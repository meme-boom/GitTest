import java.util.Arrays;

public class Bus16 {

	
	public static void main(String[] args) {

	
		int sum=0;
		for(int i=1; i<=100; i++) {
			sum+=i;
		}
		System.out.println(sum);
		float avg =sum/100;
		System.out.printf("%5f",avg);
		
		int count=0;
		int summ=0;
		for(int j=1; j<=100; j++) {
			if(j%2==0 && j%7==0) {
				summ+=j;
				count+=1;
			}
			else
				continue;
		}
		System.out.println(summ);
		
}
}