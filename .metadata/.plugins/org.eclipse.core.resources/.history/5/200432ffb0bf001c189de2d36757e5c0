

public class p1 {

	
	public static void main(String[] args) {

		int[][] arr = {
				{5,5,5,5,5,},
				{10,10,10,10,10},
				{20,20,20,20,20},
				{30,30,30,30,30}
		};
		
		int total=0;
		float avg=0f;
		
		for(int[] tmp : arr) {
			for(int i : tmp) {
				total+=i;
			}
		}
		avg=(float)total/(arr.length*arr[0].length);
		System.out.println(total);	
		System.out.println(avg);	
}
}