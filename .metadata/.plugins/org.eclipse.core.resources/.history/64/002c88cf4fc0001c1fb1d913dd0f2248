import java.util.Arrays;

public class Bus16 {

	
	public static void main(String[] args) {

	
		int sum=0;
		for(int i=1; i<=100; i++) {
			sum+=i;
		}
		System.out.println(sum);
		float avg =sum/100;
		System.out.printf("%5f",avg);
		
		int count=0;
		int sum1=0;
		for(int i=1; i<=100; i++) {
			if(i%2==0 && i%7==0) {
				count++;
				sum1+=i;
			}
			else
				continue;
		}
		System.out.println(count);
		System.out.println(sum1);
		
}
}