import java.util.Arrays;

public class Ex05 {

	
	public static void main(String[] args) {

	
		int[] arr = {9,6,7,3,5};
		int min=0;
		int i=0;
		int w=0;
		for(; i<arr.length-1; i++) {
			if(arr[i]<arr[i+1]) {
				min=arr[i];
				w=i;
			}
			arr[i]=arr[w];
		}
		
		System.out.println(min);
		System.out.println(Arrays.toString(arr)));
}
}