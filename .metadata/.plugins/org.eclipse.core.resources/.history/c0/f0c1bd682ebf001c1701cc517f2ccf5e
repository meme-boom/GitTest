import java.util.Scanner;

public class Bus8 {

	
	public static void main(String[] args) {

		String[] days= {"일","월","화","수","목","금","토"};
		
		for(int i=0; i<days.length; i++) {
			System.out.print(days[i]+" ");
		}
		System.out.println();
		
		for(int i=1; i<days.length+1; i++) {
			System.out.print(i*1+" ");
		}
}
}