import java.util.Arrays;
import java.util.Scanner;

public class d3 {

	
	public static void main(String[] args) {

		int[][] score = {
				{100,100,100},
				{20,20,20},
				{30,30,30},
				{40,40,40},
				{50,50,50},
		};
		int korT=0;
		int engT=0;
		int mathT=0;
		
		System.out.println("번호 국어 영어 수학 총점 평균 ");
		System.out.println("============================");
		
		for(int i=0; i<score.length; i++) {
			int sum=0;
			float avg= 0f;
			
			korT+=score[i][0];
			engT+=score[i][1];
			mathT+=score[i][2];
			System.out.printf("%3d",i+1);
		}
		

}
}