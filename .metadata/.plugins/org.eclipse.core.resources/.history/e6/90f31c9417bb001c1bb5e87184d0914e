import java.util.Arrays;
import java.util.Scanner;

public class Hello3 {

	
	public static void main(String[] args) {
		
		int[] abc= new int[5];
		for(int i=0; i<abc.length; i++)
			abc[i]=(int)(Math.random()*100)+1;
		System.out.println(Arrays.toString(abc));
				
		int min=100;
		int min2, min3, min4, min5;
		for(int i=0; i<abc.length; i++) {
			if(min>abc[i])
				min=abc[i];
		}
		int j=0;
		for(; j<abc.length; j++)
			if(abc[j]==min)
				break; System.out.println(j);
		int[] abc1= new  int[5];		
		System.arraycopy(abc, j, abc1, 0, j);
		}
		
		
			
		
}
