public class Bus12 {

	
	public static void main(String[] args) {

	
		int[] coinUnit = {500,100,50,10};
		int money =500;
		
		for(int i=0; i<coinUnit.length; i++) {
			int sum=0;
			while(true) {
				if(money>=coinUnit[i]) {
					money-=coinUnit[i];
					sum++;
					continue;
				}
				else
					System.out.print("필요한 "+coinUnit[i]+"원의 수는?  ");
					break;
			}
			System.out.println(sum+"개");
		}
		
		
}
}