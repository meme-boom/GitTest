import java.util.Arrays;

public class Ex05 {

	
	public static void main(String[] args) {

	
		int[] arr = {9,6,7,3,5};
		int min=100;
		int w=0;
		for(int j=0; j<arr.length-1; j++) {
			for(int i=0; i<arr.length-1; i++) {
				if(min>arr[i]) {
					min=arr[i];
					w=i;
			}
					System.out.println(w);
		} 
			int tmp= arr[j];
			arr[j]=arr[w];
			arr[w]=tmp;
		}
		System.out.println(w);
		System.out.println(Arrays.toString(arr));
}
}